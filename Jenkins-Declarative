pipeline{
    agent any
    environment{
        PATH="/opt/apache-maven/bin/mvn:$PATH"
    }
    stages{
        stage("Welcome"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/ashish456747/demo'
            }
        }
        stage("Maven Built"){
            steps{
               sh 'mvn clean package'
            }
        }
    }
}
